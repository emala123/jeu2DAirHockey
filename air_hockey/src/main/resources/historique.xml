<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>



<VBox xmlns:fx="http://javafx.com/fxml"
    fx:controller="air_hockey.frontal.vues.VueHistorique" 
    styleClass="conteneur"
    id="vue-historique">

<HBox>

   <Button 
		fx:id="boutonRetourMenu"
        text="%retourMenu"
        styleClass="gros-bouton"
        id="bouton-retour-menu">
    </Button>

</HBox>

     <Pane styleClass="petit-v"/>

     <Label text="%titre"
			styleClass="titre"
			id="titre"/>
		


     <Pane styleClass="petit-v"/>


    <ImageView>     
        <image>

            <Image url="@logo.png"/>
			
        </image>
    </ImageView>


<Pane styleClass="petit-v"/>

<Label text="%lesParties" styleClass="moyen-texte"/>

<Label 
    fx:id="labelParties"
    styleClass="petit-texte"
    id="label-message"
    text="[labelParties]">
</Label>

     <Pane styleClass="petit-v"/>

<Pane styleClass="petit-v"/>

<Button 
        text="%add"
        styleClass="add-bouton"
        id="bouton-add">
    </Button>

<Pane styleClass="petit-v"/>

<Button 
        text="%del"
        styleClass="del-bouton"
        id="bouton-del">
    </Button>

     <Pane styleClass="petit-v"/>


</VBox>